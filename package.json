        "@reduxjs/toolkit": "^1.8.6",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^27.5.2",
        "@types/node": "^17.0.45",
        "@web3-react/core": "^6.1.9",
        "@web3-react/injected-connector": "^6.0.7",
        "@web3-react/walletconnect-connector": "^6.2.13",
        "antd": "^5.0.0",
        "bignumber.js": "^9.1.0",
        "buffer": "^6.0.3",
        "cross-env": "^7.0.3",
        "date-fns": "^2.29.3",
        "ethers": "^5.7.1",
        "lodash": "^4.17.21",
        "process": "^0.11.10",
        "react": "^18.2.0",
        "react-app-rewired": "^2.2.1",
        "react-dom": "^18.2.0",
        "react-quill": "^2.0.0",
        "react-redux": "^8.0.4",
        "react-router-dom": "^6.4.2",
        "react-scripts": "5.0.1",
        "typescript": "^4.8.4",
        "util": "^0.12.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "cross-env GENERATE_SOURCEMAP=false react-app-rewired start",
        "build": "GENERATE_SOURCEMAP=false env-cmd -f .env.development react-app-rewired build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "rules": {
            "react-hooks/exhaustive-deps": [
                "warn",
                {
                    "additionalHooks": "useDataSource"
                }
            ],
            "import/no-anonymous-default-export": [
                2,
                {
                    "allowObject": true,
                    "allowLiteral": true
                }
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/lodash": "^4.14.186",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "autoprefixer": "^10.4.12",
        "postcss": "^8.4.18",
        "prettier": "^2.7.1",
        "tailwindcss": "^3.2.0"
    }
}
